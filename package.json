{
  "name": "checkip",
  "private": true,
  "version": "0.10.0",
  "scripts": {
    "lint:css": "stylelint \"frontend/**/*.scss\"",
    "lint:js": "eslint \"frontend/**/*.js\"",
    "lint:ruby": "bundle exec rubocop --parallel",
    "lint": "npm-run-all --parallel lint:*",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.rb": [
      "rubocop --force-exclusion"
    ],
    "frontend/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "frontend/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@material/button": "^13.0.0",
    "@material/drawer": "^13.0.0",
    "@material/elevation": "^13.0.0",
    "@material/icon-button": "^13.0.0",
    "@material/layout-grid": "^13.0.0",
    "@material/list": "^13.0.0",
    "@material/top-app-bar": "^13.0.0",
    "@material/typography": "^13.0.0",
    "@rails/ujs": "^6.1.4",
    "@rails/webpacker": "5.4.3",
    "resolve-url-loader": "^4.0.0",
    "sass": "1.42.0",
    "sass-loader": "^10.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "webpack-dev-server": "^3.11.3"
  }
}
