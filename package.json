{
  "name": "checkip",
  "private": true,
  "version": "0.15.0",
  "scripts": {
    "lint:css": "stylelint \"frontend/**/*.scss\"",
    "lint:js": "eslint \"frontend/**/*.js\"",
    "lint:ruby": "bundle exec rubocop --parallel",
    "lint": "npm-run-all --parallel lint:*",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build": "webpack --config webpack.config.js"
  },
  "lint-staged": {
    "*.rb": [
      "rubocop --force-exclusion"
    ],
    "frontend/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "frontend/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@material/button": "^14.0.0",
    "@material/drawer": "^14.0.0",
    "@material/elevation": "^14.0.0",
    "@material/icon-button": "^14.0.0",
    "@material/layout-grid": "^14.0.0",
    "@material/list": "^14.0.0",
    "@material/top-app-bar": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@rails/ujs": "^7.0.1",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.5.1",
    "mini-css-extract-plugin": "^2.5.2",
    "sass": "^1.49.0",
    "sass-loader": "^13.0.0",
    "webpack": "^5.67.0",
    "webpack-cli": "^5.0.1",
    "webpack-remove-empty-scripts": "^1.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-prettier": "^2.0.0"
  }
}
