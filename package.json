{
  "name": "checkip",
  "private": true,
  "version": "0.15.0",
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs rollup.config.js",
    "lint:css": "stylelint \"frontend/**/*.scss\"",
    "lint:js": "eslint \"frontend/**/*.js\"",
    "lint:ruby": "bundle exec rubocop --parallel",
    "lint": "npm-run-all --parallel lint:*",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.rb": [
      "rubocop --force-exclusion"
    ],
    "frontend/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "frontend/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@material/button": "^14.0.0",
    "@material/drawer": "^14.0.0",
    "@material/elevation": "^14.0.0",
    "@material/icon-button": "^14.0.0",
    "@material/layout-grid": "^14.0.0",
    "@material/list": "^14.0.0",
    "@material/top-app-bar": "^14.0.0",
    "@material/typography": "^14.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "postcss": "^8.4.21",
    "rollup": "^3.23.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "stylelint": "^15.3.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0"
  }
}
