{
  "name": "checkip",
  "private": true,
  "version": "0.8.0",
  "scripts": {
    "lint:css": "stylelint \"frontend/**/*.scss\"",
    "lint:js": "eslint \"frontend/**/*.js\"",
    "lint:ruby": "bundle exec rubocop --parallel",
    "lint": "npm-run-all --parallel lint:*",
    "changelog:build": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "lint-staged": {
    "*.rb": [
      "rubocop --force-exclusion"
    ],
    "frontend/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "frontend/**/*.scss": [
      "stylelint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@material/elevation": "^12.0.0",
    "@material/icon-button": "^12.0.0",
    "@material/layout-grid": "^12.0.0",
    "@material/top-app-bar": "^12.0.0",
    "@material/tooltip": "^12.0.0",
    "@material/typography": "^12.0.0",
    "@rails/ujs": "6.1.4",
    "@rails/webpacker": "5.4.0",
    "resolve-url-loader": "^4.0.0",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-prettier": "^1.2.0",
    "webpack-dev-server": "^3.11.2"
  }
}
